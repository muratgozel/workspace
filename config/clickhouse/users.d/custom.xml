<clickhouse>
  <profiles>
    <default>
      <background_merges_mutations_concurrency_ratio>1</background_merges_mutations_concurrency_ratio>
    </default>
    <throttled_all>
      <background_merges_mutations_concurrency_ratio>1</background_merges_mutations_concurrency_ratio>
      <max_threads>1</max_threads>
      <max_insert_threads>1</max_insert_threads>
      <use_uncompressed_cache>0</use_uncompressed_cache>
      <min_insert_block_size_rows>10000</min_insert_block_size_rows>
      <max_bytes_before_external_group_by>0</max_bytes_before_external_group_by>
      <max_bytes_before_external_sort>0</max_bytes_before_external_sort>
      <query_profiler_real_time_period_ns>0</query_profiler_real_time_period_ns>
      <query_profiler_cpu_time_period_ns>0</query_profiler_cpu_time_period_ns>
      <!-- turn off most per-query/thread logging -->
      <log_queries>0</log_queries>
      <log_query_threads>0</log_query_threads>
      <!-- cut processor-level profiling noise -->
      <log_processors_profiles>0</log_processors_profiles>
    </throttled_all>
  </profiles>
</clickhouse>